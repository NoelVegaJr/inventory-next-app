generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Namespace {
  id   Int    @id @default(autoincrement())
  name String
  items Item[]
}

model Item {
  id       Int    @id @default(autoincrement())
  name     String
  size     Float
  quantity Int
  location String
  namespace Namespace? @relation(fields: [namespaceId], references: [id])
  namespaceId Int? 
}
